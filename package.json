{
  "name": "har-to-openapi",
  "version": "0.0.0-development",
  "license": "MIT",
  "author": "Jonluca DeCaro <hartoopenapi@jonlu.ca>",
  "description": "Convert a HAR object to an OpenAPI object",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "package.json",
    "tsconfig.json"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "keywords": [
    "har",
    "openapi",
    "json",
    "api",
    "converter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonluca/har-to-openapi.git"
  },
  "bugs": {
    "url": "https://github.com/jonluca/har-to-openapi/issues"
  },
  "homepage": "https://github.com/jonluca/har-to-openapi",
  "scripts": {
    "prepublishOnly": "yarn build",
    "lint": "eslint src",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.+(js|jsx|ts|tsx|har|json|css|md)\"",
    "test": "vitest --coverage",
    "test:update": "vitest -u",
    "test:watch": "vitest -w"
  },
  "dependencies": {
    "@loopback/openapi-v3-types": "^1.2.1",
    "@openapi-contrib/json-schema-to-openapi-schema": "^4.2.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "quicktype-core": "^23.2.6",
    "typescript-eslint": "^8.42.0",
    "uuid": "^11.1.0",
    "whatwg-mimetype": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@types/eslint": "9.6.1",
    "@types/har-format": "^1.2.16",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.1",
    "@types/sort-json": "^2.0.3",
    "@types/whatwg-mimetype": "^3.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "c8": "^10.1.3",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "lint-staged": "^16.1.6",
    "openapi-schema-validator": "^12.1.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix"
    ],
    "**/*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "packageManager": "yarn@4.9.4"
}
