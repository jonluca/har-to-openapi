{
  "name": "har-to-openapi",
  "version": "0.0.0-development",
  "license": "MIT",
  "author": "Jonluca DeCaro <hartoopenapi@jonlu.ca>",
  "description": "Convert a HAR object to an OpenAPI object",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "package.json",
    "tsconfig.json"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "keywords": [
    "har",
    "openapi",
    "json",
    "api",
    "converter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonluca/har-to-openapi.git"
  },
  "bugs": {
    "url": "https://github.com/jonluca/har-to-openapi/issues"
  },
  "homepage": "https://github.com/jonluca/har-to-openapi",
  "scripts": {
    "prepublishOnly": "yarn build",
    "lint": "eslint src",
    "build": "rm -fr dist/* && tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.+(js|jsx|ts|tsx|har|json|css|md)\"",
    "test": "vitest --coverage",
    "test:update": "vitest -u",
    "test:watch": "vitest -w"
  },
  "dependencies": {
    "@loopback/openapi-v3-types": "^1.2.1",
    "@openapi-contrib/json-schema-to-openapi-schema": "^3.0.2",
    "globals": "^15.12.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "quicktype-core": "^23.0.170",
    "sort-json": "^2.0.1",
    "typescript-eslint": "^8.14.0",
    "uuid": "^11.0.3",
    "whatwg-mimetype": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@types/eslint": "9.6.1",
    "@types/har-format": "^1.2.16",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.9.0",
    "@types/sort-json": "^2.0.3",
    "@types/whatwg-mimetype": "^3.0.2",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^2.1.5",
    "c8": "^10.1.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-x": "^4.4.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "lint-staged": "^15.2.10",
    "openapi-schema-validator": "^12.1.3",
    "pkg-pr-new": "^0.0.37",
    "prettier": "^3.3.3",
    "typescript": "5.6.3",
    "vitest": "^2.1.5"
  },
  "overrides": {
    "tsconfig-paths": {
      "json5": "^1.0.2"
    }
  },
  "resolutions": {
    "json5": "^1.0.2"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix"
    ],
    "**/*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  }
}
